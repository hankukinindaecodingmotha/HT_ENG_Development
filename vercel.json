{
    "version": 2,
    "builds": [
        {
            "src": "Server/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "Web_UI/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "Server/server.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "Web_UI/Assesets/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "Web_UI/Assesets/Image/$1"
        },
        {
            "src": "/(.*)",
            "dest": "Web_UI/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "domains": [
        "hteng.co.kr",
        "www.hteng.co.kr"
    ]
}